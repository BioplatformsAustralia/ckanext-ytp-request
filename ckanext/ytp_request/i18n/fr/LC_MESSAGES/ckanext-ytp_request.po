#: ckanext/ytp_request/mail.py:10
#, python-format
msgid "New membership request (%(organization)s)"
msgstr "Nouvelle demande d’adhésion (%(organization)s)"

#: ckanext/ytp_request/mail.py:15
#, python-format
msgid ""
"        User %(user)s (%(email)s) has requested membership to "
"organization %(organization)s.\n"
"\n"
"        %(link)s\n"
"\n"
"Regards,\n"
"%(sitename)s\n"
"%(siteemail)s\n"
msgstr ""
"L’utilisateur %(user)s (%(email)s) a demandé une adhésion auprès de "
"l’organisation %(organization)s.\n"
"\n"
"%(link)s\n"
"\n"
"Cordialement,\n"
"%(sitename)s\n"
"%(siteemail)s\n"

#: ckanext/ytp_request/mail.py:26
#, python-format
msgid "Organization membership approved (%(organization)s)"
msgstr "adhésion auprès de l’organisation approuvée (%(organization)s)"

#: ckanext/ytp_request/mail.py:32
#, python-format
msgid ""
"        Your membership request to organization %(organization)s with "
"%(role)s\n"
"        access has been approved.\n"
"\n"
"        Regards,\n"
"        %(sitename)s\n"
"        %(siteemail)s\n"
"        "
msgstr ""
"        Votre demande d’adhésion auprès de l’organisation "
"%(organization)s en tant que %(role)s\n"
"        a été approuvée.\n"
"\n"
"        Cordialement,\n"
"        %(sitename)s\n"
"        %(siteemail)s\n"
"        "

#: ckanext/ytp_request/mail.py:44
#, python-format
msgid "Organization membership rejected (%(organization)s)"
msgstr "adhésion auprès de l’organisation refusée (%(organization)s)"

#: ckanext/ytp_request/mail.py:50
#, python-format
msgid ""
"        Unfortunately your membership request to organization "
"%(organization)s\n"
"        with %(role)s access has been rejected.  If you think this was a\n"
"        mistake, please contact the organisation's administrator "
"directly.\n"
"\n"
"        Regards,\n"
"        %(sitename)s\n"
"        %(siteemail)s\n"
"        "
msgstr ""
"        Malheureusement, votre demande d’adhésion auprès de l’organisation %(organization)s\n"
"        en tant que %(role)s a été refusée. Si vous estimez que c’est une erreur,\n"
"        veuillez vous adresser à l’administrateur de votre organisation.\n"
"\n"
"        Cordialement,\n"
"        %(sitename)s\n"
"        %(siteemail)s\n"
"        "

